	   /*
       /* 获取CKEditorFuncNum
       */
		function getUrlParam(url)   
        {   
            var reParam = new RegExp('(?:[\?&]|&amp;)CKEditorFuncNum=([^&]+)', 'i') ;   
            var match = url.match(reParam) ;   
    
            return (match && match.length > 1) ? match[1] : '' ;   
        }   


